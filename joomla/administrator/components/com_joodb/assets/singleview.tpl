<!-- JooDatabase: initial template for new databases  -->
<!-- see media/com_joodb/assets/joodb.css for style definitions -->
<div class="block header">
    <div style="float: right; width: 100px; text-align: right; margin-left: 10px;">{joodb printbutton}</div>
    <h3 class="title">#S_DEFAULT_TITLE</h3>
</div>
<div class="block odd">
    <p><span class="small">#S_DEFAULT_DATE</span></p>
    <div style="float:right; margin: 0 0 10px 10px; ">{joodb thumb}</div>
    #S_DEFAULT_CONTENT
</div>
<div class="block">
    #S_DEFAULT_FIELDS
</div>
<div class="block odd">{joodb backbutton}</div>
